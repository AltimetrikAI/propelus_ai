{
  "name": "@propelus/lambda-ingestion-cleansing",
  "version": "2.0.0",
  "description": "Combined Bronze ingestion and Silver processing Lambda - TypeScript implementation",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "package": "npm run build && cd dist && zip -r ../function.zip .",
    "deploy": "npm run package && aws lambda update-function-code --function-name ingestion-cleansing --zip-file fileb://function.zip",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:generate": "ts-node test/sample-data-generator.ts",
    "test:validate": "ts-node test/validate-test-data.ts",
    "test:local": "ts-node test/local-test-runner.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "pg": "^8.11.3",
    "xlsx": "^0.18.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "propelus",
    "taxonomy",
    "ingestion",
    "lambda",
    "bronze",
    "silver",
    "aws"
  ],
  "author": "Propelus AI Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  }
}
