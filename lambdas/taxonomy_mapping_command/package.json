{
  "name": "taxonomy-mapping-command",
  "version": "1.0.0",
  "description": "Lambda function for Translation Constant Command Logic - Non-AI rule-based taxonomy mapping",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "package": "npm run build && zip -r function.zip dist node_modules",
    "clean": "rm -rf dist function.zip"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
